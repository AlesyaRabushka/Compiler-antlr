main{
queue example_q1 = [6, 7, 8, 9, 11];
queue example_q2 = [10, 11, 12];

element k = input();
switch (k){
    case 1{
        example_q1 = example_q1 + example_q2;
    }
    case 2{
        example_q1 = example_q1 - example_q2;
    }
    case 3{
        example_q1 = example_q1 * example_q2;
    }
    case 4{
        example_q1 = example_q1 / example_q2;
    }
    case 5{
        list example_q3 = example_q1.merge(example_q2);
    }
};
};